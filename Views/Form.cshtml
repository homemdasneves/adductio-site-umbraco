@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.Form>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "Master.cshtml";

	var formId = Model.Value<string>("trainingForm");
	var parsedFormId = Guid.Empty;

	if (formId != null)
	{
		parsedFormId = Guid.Parse(formId);
	}
}
<section class="mt-20">
	<div class="container">
		<h1 class="text-adductioBlue text-4xl font-bold">@Model.Name</h1>
	</div>
</section>
<section class="form form-training mb-20">
	<div class="container pt-20 pb-10">
		<div class="px-4 md:px-0">
			@await Component.InvokeAsync("RenderForm", new
				{
					formId = @parsedFormId,
					theme = "default",
					includeScripts = false
				})
		</div>
	</div>
</section>