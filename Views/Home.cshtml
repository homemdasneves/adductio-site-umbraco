@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.Home>

@{
	Layout = "Master.cshtml";
}

@await Html.PartialAsync("home/HomeCarousel", Model)
@await Html.PartialAsync("home/Services")
@await Html.PartialAsync("home/Clients")
@await Html.PartialAsync(
	"home/Split",
	Model, new ViewDataDictionary(ViewData) {
    { "className", "bg-adductioSoftGrey pt-20 pb-20" }
})
@await Html.PartialAsync("global/ContactForm")
