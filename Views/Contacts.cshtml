@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage;

@{
	Layout = "Master.cshtml";
}

<section class="mt-20">
	<div class="container">
		<h1 class="text-adductioBlue text-5xl font-bold">@Model.Name</h1>
	</div>
</section>
<section class="my-10">
	<div class="container">
		<div class="relative pb-[50%] -ml-10 -mr-10">
			<iframe class="absolute inset-0 w-full h-full" width="100" height="auto" style="border:0" loading="lazy"
				allowfullscreen referrerpolicy="no-referrer-when-downgrade" src="https://www.google.com/maps/embed/v1/place?key=API_KEY
            &q=Space+Needle,Seattle+WA">
			</iframe>
		</div>
	</div>
</section>
@await Html.PartialAsync("global/ContactForm")